   }\r\n}\r\n//action module favorite\r\nexport const actions = {\r\n    addFavorite({ commit }, favorite) {\r\n        commit('ADD_FAVORITES', favorite)\r\n    },\r\n    remove({commit}, favoriteToRemove){\r\n        commit('DELETE', favoriteToRemove)\r\n    }\r\n}"]}]}